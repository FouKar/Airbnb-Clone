<table class="table">
  <thead class="thead-dark">
    <tr>
      <th></th>
      <th scope="col">Title</th>

      <th scope="col">Price</th>
      <th scope="col">Location</th>
      <th scope="col">Manage</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
      <tr>
        <td><img
            src="/{{this.file}}"
            height="100px"
            width="100px"
            alt=""
          /></td>
        <td>{{this.title}}</td>
        <td>{{this.price}}</td>
        <td>{{this.city}}, {{this.province}}, {{this.country}}</td>
        <td>
          <a
            href="/room/edit/{{this.id}}"
            class="btn btn-primary edit-delete-button"
          >Edit</a>
          <a
            href="/room/delete/{{this.id}}?method=DELETE"
            class="btn btn-danger edit-delete-button"
          >Delete</a></td>
      </tr>
    {{else}}
      <td colspan="5">
        <p>No Rooms in the database</p>
      </td>

    {{/each}}
  </tbody>
</table>
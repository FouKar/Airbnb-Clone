      <form action="/User/reg" method="POST">
        <div class="form-group d-flex flex-column mr-5 ml-5 mt-4">
          <input
            type="email"
            class="form-control pb-3 pt-3 pl-3 mt-3"
            name="emailLogReg"
            id="emailInputF"
            aria-describedby="emailHelp"
            placeholder="Enter email"
            autocomplete="email"
          />
          {{#if eMErr}}
            <small id="emailHelpBlock" class="form-text text-danger">
              {{eMErr}}
            </small>
          {{/if}}

          <input
            type="text"
            class="form-control pb-3 pt-3 pl-3 mt-3"
            name="fName"
            id="fNmT"
            placeholder="First Name"
          />
          {{#if fNmErr}}
            <small
              id="firstnameHelpBlock"
              class="ml-2 d-flex flex-wrap flex-row form-text text-danger"
            >
              {{fNmErr}}
            </small>
          {{/if}}

          <input
            type="text"
            class="form-control pb-3 pt-3 pl-3 mt-3"
            name="lName"
            id="lNmT"
            placeholder="Last Name"
          />
          {{#if lNmErr}}
            <small
              id="lastnameHelpBlock"
              class="d-flex flex-wrap flex-row form-text text-danger"
            >
              {{lNmErr}}
            </small>
          {{/if}}

          <input
            type="tel"
            class="form-control pb-3 pt-3 pl-3 mt-3"
            name="phone"
            id="phoneNumT"
            placeholder="Enter Phone Number"
            autocomplete="tel"
          />
          {{#if phoneErr}}
            <small id="telephoneHelpBlock" class="form-text text-danger">
              {{phoneErr}}
            </small>
          {{/if}}

          <input
            type="password"
            class="form-control pb-3 pt-3 pl-3 mt-3"
            name="passLogReg"
            id="passInputFive"
            placeholder="Create a password"
            autocomplete="current-password"
          />
          {{#if passErr}}

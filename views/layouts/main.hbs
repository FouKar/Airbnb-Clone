{{>header}}
<div class="modal" tabindex="-1" id="loginModal" role="dialog">
<div class="modal-dialog modal-dialog-centered " role="document">
        <div class="modal-content rounded">
            <div class="modal-header ">
                <h5 class="modal-title pt-2 mx-auto">Log In Account</h5>
            </div>
            <div class="modal-body">
                <h4 class = "mt-3 text-center" style="color: #C82433 !important;">Sign in to Igloo and Book your Room!</h4>
    
                <form action="/User/log" method="POST">
                    <div  class="form-group d-flex flex-column mr-5 ml-5 mt-5">
                        <input type="email" class="form-control pb-3 pt-3 pl-3" name="emailLog" id="emailInputFour"
                            aria-describedby="emailHelp" placeholder="Enter email" 
                            >
          
    
                        <input type="password" class="form-control pb-3 pt-3 pl-3 mt-3" name="passLog" id="passInputFive"
                            placeholder="Enter password"  >
                   
                        <a  href="/" class="btn btn-link text-left" style="text-decoration: none !important;" id="forgotPass">Forgot your password?</a>    
                        <input type="submit"  value="SIGN IN" class="btn btn-danger pt-3 pb-3 mb-3 "></input>
                        <button type="button" data-toggle="modal" data-target="#registerModal" data-dismiss="modal" class="btn btn-outline-danger pt-3 pb-3 signinC">Create a New Account</button>
    
                <div class="d-flex flex-row justify-content-between mt-4">
                    <button type="button submit" class="btn pt-3 pb-3 "
                        style="color:white !important; background-color: #DB4437 !important;"><svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-google" viewBox="0 0 16 16">
                      <path d="M15.545 6.558a9.42 9.42 0 0 1 .139 1.626c0 2.434-.87 4.492-2.384 5.885h.002C11.978 15.292 10.158 16 8 16A8 8 0 1 1 8 0a7.689 7.689 0 0 1 5.352 2.082l-2.284 2.284A4.347 4.347 0 0 0 8 3.166c-2.087 0-3.86 1.408-4.492 3.304a4.792 4.792 0 0 0 0 3.063h.003c.635 1.893 2.405 3.301 4.492 3.301 1.078 0 2.004-.276 2.722-.764h-.003a3.702 3.702 0 0 0 1.599-2.431H8v-3.08h7.545z"/>
                    </svg> Login Google</button>
                        <button type="button submit" class="btn pt-3 pb-3 "
                        style="color:white !important; background-color: #3b5998 !important;"><svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-facebook" viewBox="0 0 16 16">
